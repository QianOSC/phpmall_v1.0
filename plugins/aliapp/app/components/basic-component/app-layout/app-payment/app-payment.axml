<view class="{{(('app-payment main-center cross-center data-v-68df6871')+' '+(showPayment?'show':''))}}"><block a:if="{{payData}}"><view class="modal data-v-68df6871"><view class="title data-v-68df6871"><view class="data-v-68df6871">支付方式</view><view data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" class="cancel data-v-68df6871" onTap="__e"><image src="/static/image/icon/close.png" class="data-v-68df6871"></image></view></view><view class="pay-amount data-v-68df6871">{{"支付金额 "+payData.amount+" 元"}}</view><view class="pay-type-list data-v-68df6871"><block a:for="{{payData.list}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['checkPayType',[index]]]]]}}" class="pay-type-item cross-center data-v-68df6871" onTap="__e"><view class="box-grow-0 data-v-68df6871"><image class="pay-type-icon data-v-68df6871" src="{{item.icon}}"></image></view><view class="box-grow-1 data-v-68df6871"><view class="pay-type-name data-v-68df6871">{{item.name}}</view><block a:if="{{item.desc}}"><view class="pay-type-desc data-v-68df6871">{{item.desc}}</view></block></view><view class="box-grow-0 data-v-68df6871"><block a:if="{{item.checked}}"><app-radio onInput="__e" vue-id="{{'1-'+index}}" theme="{{getTheme}}" type="round" value="{{item.checked}}" data-event-opts="{{[['^input',[['__set_model',['$0','checked','$event',[]],[[['payData.list','',index]]]]]]]}}" class="data-v-68df6871" onVueInit="__l"></app-radio></block></view></view></block></view><view class="footer data-v-68df6871"><app-button vue-id="2" type="important" theme="{{getTheme}}" round data-event-opts="{{[['^click',[['confirm']]]]}}" onClick="__e" class="data-v-68df6871" onVueInit="__l">提交订单</app-button></view></view></block></view>