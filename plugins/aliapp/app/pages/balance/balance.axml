<app-layout vue-id="1" class="data-v-1e251dcc" onVueInit="__l"><view class="balance-head dir-top-nowrap cross-center data-v-1e251dcc" style="{{'background-image:'+('url('+setting.bj_pic_url.url+')')+';'}}"><image class="balance-legend data-v-1e251dcc" src="{{setting.re_pic_url.url?setting.re_pic_url.url:'/static/image/icon/question.png'}}" data-event-opts="{{[['tap',[['getRules',['$event']]]]]}}" onTap="__e"></image><view class="account data-v-1e251dcc">账户余额(元)</view><view class="balance data-v-1e251dcc">{{balance}}</view><view class="app-button data-v-1e251dcc"><app-form-id vue-id="{{('2')+','+('1')}}" class="data-v-1e251dcc" onVueInit="__l"><app-button vue-id="{{('3')+','+('2')}}" background="rgba(255,255,255,0)" color="#FFFFFF" round height="56" data-event-opts="{{[['^click',[['recharge']]]]}}" onClick="__e" class="data-v-1e251dcc" onVueInit="__l">{{setting.re_name+''}}</app-button></app-form-id></view></view><block a:if="{{setting.ad_pic_url}}"><app-jump-button vue-id="{{('4')+','+('1')}}" open_type="{{setting.open_type}}" url="{{setting.page_url}}" params="{{setting.params}}" class="data-v-1e251dcc" onVueInit="__l"><image class="balance-ad data-v-1e251dcc" src="{{setting.ad_pic_url.url}}"></image></app-jump-button></block><view class="balance-date main-center cross-center data-v-1e251dcc"><view data-event-opts="{{[['tap',[['dateLess',['$event']]]]]}}" class="icon-view cross-center main-center data-v-1e251dcc" onTap="__e"><image class="balance-icon data-v-1e251dcc" src="/static/image/icon/arrow-left.png"></image></view><picker mode="date" value="{{date}}" fields="month" data-event-opts="{{[['change',[['dateChange',['$event']]]]]}}" onChange="__e" class="data-v-1e251dcc"><view class="data-v-1e251dcc">{{date_a}}</view></picker><view data-event-opts="{{[['tap',[['datePlus',['$event']]]]]}}" class="icon-view cross-center main-center data-v-1e251dcc" onTap="__e"><image class="balance-icon data-v-1e251dcc" src="/static/image/icon/arrow-right.png"></image></view></view><block a:for="{{logs}}" a:for-item="item" a:for-index="index" a:key="index"><block class="data-v-1e251dcc"><app-form-id vue-id="{{('5-'+index)+','+('1')}}" open-type="navigate" data-event-opts="{{[['^click',[['detail',['$0'],[[['logs','',index]]]]]]]}}" onClick="__e" class="data-v-1e251dcc" onVueInit="__l"><view class="dir-left-nowrap cross-center balance-list data-v-1e251dcc"><view class="box-grow-1 dir-top-nowrap data-v-1e251dcc"><view class="balance-desc dir-left-nowrap t-omit data-v-1e251dcc">{{item.desc}}</view><view class="balance-time data-v-1e251dcc">{{item.created_at}}</view></view><block a:if="{{item.type==1}}"><view class="box-grow-0 balance-plus data-v-1e251dcc">{{"+"+item.money}}</view></block><block a:if="{{item.type==2}}"><view class="box-grow-0 balance-less data-v-1e251dcc">{{"-"+item.money}}</view></block></view></app-form-id></block></block></app-layout>