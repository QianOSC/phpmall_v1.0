<app-layout vue-id="1" class="data-v-1f32dfc0" onVueInit="__l"><block a:if="{{pageShow}}"><view class="dir-top-nowrap recharge data-v-1f32dfc0"><view class="account data-v-1f32dfc0">我的账户</view><view class="dir-left-nowrap bg cross-center data-v-1f32dfc0" style="{{'background-image:'+('url('+appImg.mall.balance_recharge+')')+';'}}"><image class="image box-grow-0 data-v-1f32dfc0" src="./image/icon-balance.png"></image><view class="balance-text box-grow-1 data-v-1f32dfc0">{{customPage.balance_title}}</view><view class="balance-price box-grow-0 data-v-1f32dfc0">{{balance}}</view></view><view class="amount-text data-v-1f32dfc0">{{customPage.recharge_amount_title}}</view><view class="dir-left-wrap select data-v-1f32dfc0"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['setPrice',[index]]]]]}}" class="{{(('dir-top-nowrap cross-center main-center box data-v-1f32dfc0')+' '+(index===selected?'active':''))}}" onTap="__e"><view class="price data-v-1f32dfc0">{{"充"+item.pay_price+"元"}}</view><block a:if="{{item.send_price>0}}"><view class="send data-v-1f32dfc0">{{"送"+item.send_price+"元"}}</view></block><block a:if="{{item.send_integral>0}}"><view class="send data-v-1f32dfc0">{{"送"+item.send_integral+"积分"}}</view></block><block a:if="{{item.member}}"><view class="send dir-left-nowrap cross-center data-v-1f32dfc0"><view class="t-omit data-v-1f32dfc0">{{"送"+item.member.name}}</view><view data-event-opts="{{[['tap',[['navMember',['$0'],[[['list','',index,'member']]]]]]]}}" class="box-grow-0 member-info main-center cross-center data-v-1f32dfc0" catchTap="__e">详情</view></view></block></view></block></view><block a:if="{{setting.type==='1'}}"><view class="{{(('input data-v-1f32dfc0')+' '+(selected===-1&&getFocus?'red':'grey'))}}"><input placeholder-style="color:#bbbbbb" type="digit" focus="{{getFocus}}" placeholder="{{'手动输入'+customPage.recharge_amount_title}}" data-event-opts="{{[['focus',[['bindFocus',['$event']]]],['blur',