<view class="app-add-subtract dir-left-nowrap main-right cross-center data-v-0489835c"><block a:if="{{total_num>0}}"><image class="app-icon data-v-0489835c" src="/static/image/quick-shop/subtract.png" data-event-opts="{{[['tap',[['subtract',['$event']]]]]}}" onTap="__e"></image></block><block a:if="{{total_num>=0}}"><input class="app-input data-v-0489835c" type="number" data-event-opts="{{[['input',[['changeNum',['$event']]]]]}}" value="{{total_num===0?'':total_num}}" onInput="__e"/></block><image hidden="{{!(loading)}}" class="{{(('app-icon data-v-0489835c')+' '+(theme+'-m-back '+theme))}}" src="/static/image/quick-shop/add.png" data-event-opts="{{[['load',[['imgLoad',['$event']]]],['tap',[['add',['$event']]]]]}}" onLoad="__e" onTap="__e"></image></view>