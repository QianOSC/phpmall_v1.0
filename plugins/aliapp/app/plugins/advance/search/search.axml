<app-layout vue-id="1" class="data-v-666f48c4" onVueInit="__l"><view class="search data-v-666f48c4" style="{{'background-color:'+(search?'#f7f7f7':'#ffffff')+';'}}"><view class="top dir-left-nowrap cross-center main-between data-v-666f48c4"><input class="input data-v-666f48c4" type="text" focus="{{true}}" data-event-opts="{{[['blur',[['request',['$event']]]],['input',[['__set_model',['','search_text','$event',[]]]]]]}}" value="{{search_text}}" onBlur="__e" onInput="__e"/><image hidden="{{!(search_text.length>0)}}" class="delete data-v-666f48c4" src="../image/delete.png" data-event-opts="{{[['tap',[['empyt_search',['$event']]]]]}}" onTap="__e"></image><text data-event-opts="{{[['tap',[['request',['$event']]]]]}}" style="color:#666666;font-size:32rpx;" onTap="__e" class="data-v-666f48c4">搜索</text></view><block a:if="{{!search}}"><view class="center data-v-666f48c4"><block a:if="{{strong.length>0}}"><view class="strong dir-left-nowrap main-between cross-center data-v-666f48c4"><text class="data-v-666f48c4">历史搜索</text><image class="delete data-v-666f48c4" src="/static/image/icon/delete.png" data-event-opts="{{[['tap',[['empty_strong',['$event']]]]]}}" onTap="__e"></image></view></block><view class="data-v-666f48c4"><block a:for="{{strong}}" a:for-item="item" a:for-index="index" a:key="index"><text data-event-opts="{{[['tap',[['search_strong',['$0'],[[['strong','',index]]]]]]]}}" class="strong-item data-v-666f48c4" onTap="__e">{{''+item+''}}</text></block></view></view></block><block a:if="{{search_list.length>0}}"><view class="bottom data-v-666f48c4"><index-product-list vue-id="{{('2')+','+('1')}}" theme="{{getTheme}}" product="{{search_list}}" class="data-v-666f48c4" onVueInit="__l"></index-product-list></view></block><block a:if="{{search_list.length===0&&search}}"><view class="order-empty data-v-666f48c4"><image src="../image/product.png" class="data-v-666f48c4"></image><text class="data-v-666f48c4">没有任何信息哦~</text></view></block></view></app-layout>