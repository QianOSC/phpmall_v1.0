<block s-if="{{coupon&&coupon.list&&coupon.list.length>0}}"><view class="app-coupon-modal main-center cross-center data-v-0598b18d"><view class="coupon-modal data-v-0598b18d"><image class="title-img data-v-0598b18d" src="{{img}}"></image><view class="get-coupon-content data-v-0598b18d"><block s-if="{{coupon.type==='receive'&&coupon.list[0].rest>=0}}"><view class="rest dir-left-nowrap main-center cross-center data-v-0598b18d"><view class="data-v-0598b18d">还剩</view><view class="rest-count data-v-0598b18d">{{coupon.list[0].rest}}</view><view class="data-v-0598b18d">次领取次数</view></view></block><block s-if="{{coupon.type==='invite'}}"><view class="invite coupon-head-label data-v-0598b18d"><view class="data-v-0598b18d">{{"成功邀请"+coupon.list[0].id+"位好友，获得奖励"}}</view></view></block><block s-else><view class="coupon-head-label data-v-0598b18d">{{"*"+labelText}}</view></block><scroll-view class="coupon-list data-v-0598b18d" scroll-y="true"><block s-for="{{$root.l0}}" s-for-item="item" s-for-index="index" s-key="index"><view class="dir-left-nowrap cross-center coupon-item data-v-0598b18d"><view class="price box-grow-0 data-v-0598b18d"><block s-if="{{item.$orig.share_type===1}}"><image src="../../../../static/image/hongbao.png" class="data-v-0598b18d"></image></block><block s-if="{{item.$orig.share_type===2}}"><image src="../../../../static/image/integral.png" class="data-v-0598b18d"></image></block><block s-if="{{item.$orig.share_type===3}}"><image class="card data-v-0598b18d" src="{{item.$orig.pic_url}}"></image></block><block s-if="{{item.$orig.share_type===4}}"><block class="data-v-0598b18d"><block s-if="{{item.$orig.type==2}}"><app-price vue-id="{{'1-'+index}}" price="{{item.$orig.sub_price}}" class="data-v-0598b18d"></app-price></block><block s-else><view class="discount data-v-0598b18d">{{item.$orig.discount}}</view></block></block></block></view><view class="right dir-top-nowrap main-center box-grow-1 data-v-0598b18d"><block s-if="{{item.g0}}"><view class="{{['data-v-0598b18d','t-omit'+(item.$orig.share_type===3?'-two':'')]}}">{{item.$orig.name+''}}</view></block><block s-else><view class="t-omit data-v-0598b18d">{{item.$orig.name}}</view></block><view class="t-omit content data-v-0598b18d">{{item.$orig.content}}</view><block s-if="{{item.$orig.discount_limit}}"><view class="content data-v-0598b18d">{{"优惠上限:￥"+item.$orig.discount_limit}}</view></block></view><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['coupon.list','',index,'page_url']]]]]]]}}" class="box-grow-0 btn data-v-0598b18d" bindtap="__e">去使用</view></view></block></scroll-view></view><view data-event-opts="{{[['tap',[['closeCouponBox',['$event']]]]]}}" class="main-center data-v-0598b18d" bindtap="__e"><image class="bottom-close data-v-0598b18d" src="../../../../static/image/icon/icon-popup-close.png"></image></view></view></view></block>