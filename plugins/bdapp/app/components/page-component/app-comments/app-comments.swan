<block s-if="{{mall.setting.is_comment==1}}"><view class="app-comments data-v-07acceda"><block s-if="{{showType!=='detail'}}"><view class="empty data-v-07acceda"><view class="app-top dir-left-nowrap cross-center data-v-07acceda"><block s-for="{{commentCount}}" s-for-item="item" s-for-index="index" s-key="index"><view data-event-opts="{{[['tap',[['clickStatus',['$0'],[[['commentCount','',index,'index']]]]]]]}}" class="{{['box box-grow-1 main-center cross-center data-v-07acceda',status===item.index?getTheme+'-m-back u-text '+getTheme:'background']}}" bindtap="__e">{{''+item.name+"("+item.count+')'}}</view></block></view></view></block><block s-if="{{list.length>0}}"><view class="list data-v-07acceda"><block s-if="{{showType==='detail'}}"><view class="dir-left-nowrap block cross-center data-v-07acceda"><view class="box-grow-1 data-v-07acceda">评价</view><view data-event-opts="{{[['tap',[['goto',['$event']]]]]}}" class="box-grow-0 more data-v-07acceda" bindtap="__e">查看更多</view><image class="box-grow-0 data-v-07acceda" src="../../../static/image/icon/arrow-right.png"></image></view></block><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><view class="{{['comments data-v-07acceda',showType==='detail'?'bt':'bb']}}"><view class="title dir-left-nowrap cross-center data-v-07acceda"><image class="box-grow-0 data-v-07acceda" src="{{item.avatar}}"></image><view class="box-grow-1 data-v-07acceda">{{item.nickname}}</view><view class="more box-grow-0 data-v-07acceda">{{item.time}}</view></view><view class="c-attr-name data-v-07acceda">{{item.attr_name}}</view><view class="{{['data-v-07acceda',showType==='detail'?'content':'']}}">{{item.content}}</view><view class="dir-left-wrap pic-list data-v-07acceda"><block s-for="{{item.pic_url}}" s-for-item="pic_url" s-for-index="pic_url_index" s-key="pic_url_index"><image src="{{pic_url}}" data-event-opts="{{[['tap',[['imgPreview',[index,pic_url_index]]]]]}}" bindtap="__e" class="data-v-07acceda"></image></block></view><block s-if="{{showType!=='detail'&&item.reply_content}}"><view class="replay data-v-07acceda"><view class="data-v-07acceda"><text class="{{['data-v-07acceda',getTheme+'-m-text '+getTheme]}}">商家：</text>{{''+item.reply_content+''}}</view></view></block></view></block></view></block><block s-else><view class="block cross-center data-v-07acceda">暂无评价</view></block></view></block>