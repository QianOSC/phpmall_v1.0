<view class="app-exclusive-coupon data-v-e7915384" style="{{'background-color:'+(''+(noneColor?'':background))+';'}}"><block s-if="{{showTop}}"><view data-event-opts="{{[['tap',[['route']]]]}}" class="app-top main-between data-v-e7915384" bindtap="__e"><view class="app-left main-between cross-center data-v-e7915384"><icon class="app-icon data-v-e7915384"></icon><text class="app-title data-v-e7915384">专享优惠券</text></view><view class="app-right main-between cross-center data-v-e7915384"><text class="app-text data-v-e7915384">更多</text><icon class="app-icon data-v-e7915384"></icon></view></view></block><view class="app-bottom data-v-e7915384"><scroll-view class="app-scroll dir-left-nowrap data-v-e7915384" scroll-x><block s-for="{{coupon_list}}" s-for-item="item" s-for-index="index" s-key="index"><view class="app-item data-v-e7915384" style="{{'background-image:'+('url('+(item.is_receive==0?unclaimedBg:item.is_receive==1?receiveBg:'')+')')+';'}}"><view data-event-opts="{{[['tap',[['receive',[index]]]]]}}" class="main-left data-v-e7915384" bindtap="__e"><view class="app-text-left data-v-e7915384"><view class="app-text-top data-v-e7915384"><block s-if="{{item.type==='1'}}"><text class="app-number discount data-v-e7915384" style="{{'color:'+(textColor)+';'}}">{{item.discount}}</text></block><block s-else><text class="app-symbol data-v-e7915384" style="{{'color:'+(textColor)+';'}}">￥</text><text class="app-number data-v-e7915384" style="{{'color:'+(textColor)+';'}}">{{item.sub_price}}</text></block></view><text class="app-text-bottom data-v-e7915384" style="{{'color:'+(textColor)+';'}}">{{"满"+item.min_price+"元可用"}}</text></view><view class="app-text-right data-v-e7915384"><text style="{{'color:'+(textColor)+';'}}" class="data-v-e7915384">{{item.is_receive==0?receiveTip:item.is_receive>0?'已领取':''}}</text></view></view></view></block></scroll-view></view></view>