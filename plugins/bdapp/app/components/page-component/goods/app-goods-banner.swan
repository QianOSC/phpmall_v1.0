<block s-if="{{picList.length>0}}"><view class="app-goods-banner data-v-91f06bfc"><swiper class="swiper data-v-91f06bfc" autoplay="{{autoplay}}" indicator-dots="{{picList.length>1}}" circular indicator-color="rgba(255,255,255,.3)" indicator-active-color="#30353c" data-event-opts="{{[['change',[['bannerChange',['$event']]]]]}}" bindchange="__e"><block s-for="{{picList}}" s-for-item="item" s-for-index="index" s-key="index"><swiper-item class="data-v-91f06bfc"><view data-event-opts="{{[['tap',[['clickImage',[index]]]]]}}" class="swiper-img data-v-91f06bfc" style="{{'background-image:'+('url('+item.pic_url+')')+';'}}" bindtap="__e"><block s-if="{{index===0&&videoUrl}}"><app-video vue-id="{{'1-'+index}}" height="750rpx" pic-url="{{item.pic_url}}" url="{{videoUrl}}" data-event-opts="{{[['^videoStart',[['videoStart']]],['tap',[['preventD',['$event']]]]]}}" bind:videoStart="__e" catchtap="__e" class="data-v-91f06bfc"></app-video></block></view></swiper-item></block></swiper><view class="data-v-91f06bfc"><block s-if="{{isCart}}"><app-jump-button vue-id="2" url="/pages/cart/cart" class="data-v-91f06bfc" vue-slots="{{['default']}}"><view class="cart data-v-91f06bfc" style="{{'top:'+(isScanQrCode?'250rpx':'60rpx')+';'}}"><image load-lazy src="../../../static/image/icon/nav-icon-cart.png" class="data-v-91f06bfc"></image></view></app-jump-button></block></view><block s-if="{{share>0}}"><view class="share-commission dir-left-nowrap cross-center data-v-91f06bfc"><image load-lazy src="../../../static/image/icon/share/share_commission.png" class="data-v-91f06bfc"></image><view class="text data-v-91f06bfc"><view class="data-v-91f06bfc">分销最高可赚</view><view class="data-v-91f06bfc">{{"￥"+share}}</view></view></view></block></view></block>