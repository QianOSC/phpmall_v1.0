<app-layout vue-id="1" class="data-v-71373bb2" vue-slots="{{['default']}}"><app-tab-nav vue-id="{{('2')+','+('1')}}" tabList="{{tabList}}" padding="0" activeItem="{{activeTab}}" theme="blue" data-event-opts="{{[['^click',[['tabStatus']]]]}}" bind:click="__e" class="data-v-71373bb2"></app-tab-nav><view class="order main-center cross-center data-v-71373bb2"><view data-event-opts="{{[['tap',[['choose',['$event']]]]]}}" class="main-center cross-center data-v-71373bb2" bindtap="__e"><view class="data-v-71373bb2">{{active.name}}</view><image class="more data-v-71373bb2" src="../image/open.png"></image></view></view><view class="order-placeholder data-v-71373bb2"></view><block s-for="{{list}}" s-for-item="item" s-for-index="__i0__" s-key="id"><view data-event-opts="{{[['tap',[['toOrder',['$0'],[[['list','id',item.id,'order_no']]]]]]]}}" bindtap="__e" class="data-v-71373bb2"><view class="msg-item data-v-71373bb2"><block s-if="{{activeTab==8}}"><view class="msg-title data-v-71373bb2"><text class="plugin-name data-v-71373bb2">{{item.plugin_name}}</text>有新订单啦</view></block><block s-if="{{activeTab==4}}"><view class="msg-title data-v-71373bb2">退款申请</view></block><block s-if="{{activeTab==1&&item.type==2}}"><view class="msg-title cross-center data-v-71373bb2"><text class="replace data-v-71373bb2"></text>换货申请</view></block><block s-if="{{activeTab==1&&item.type==1}}"><view class="msg-title cross-center data-v-71373bb2"><text class="return data-v-71373bb2"></text>退货退款申请</view></block><view class="msg-time data-v-71373bb2">{{item.created_at}}</view><view class="goods-info data-v-71373bb2"><image class="goods-img data-v-71373bb2" src="{{activeTab==1?item.detail.goods_info.goods_attr.cover_pic:item.detail[0].goods.goodsWarehouse.cover_pic}}"></image><view class="goods-name t-omit-two data-v-71373bb2"><text class="data-v-71373bb2">{{activeTab==1?item.detail.goods_info.goods_attr.name:item.detail[0].goods.goodsWarehouse.name}}</text><block s-if="{{item.detail.length>1&&activeTab!=1}}"><text class="data-v-71373bb2">{{"等"+item.detail.length+"件商品"}}</text></block></view><view class="goods-user data-v-71373bb2">{{"来自用户"+item.nickname}}</view></view></view></view></block><block s-if="{{list.length===0&&!loading}}"><view class="no-tip data-v-71373bb2"><image src="{{adminImg.no_message}}" class="data-v-71373bb2"></image><view class="data-v-71373bb2">没有任何消息哦~</view></view></block><block s-if="{{dialog}}"><view class="dialog data-v-71373bb2"><view class="picker-list data-v-71373bb2"><view class="main-between picker-header data-v-71373bb2"><view data-event-opts="{{[['tap',[['toggle',[false,2]]]]]}}" bindtap="__e" class="data-v-71373bb2">取消</view><view data-event-opts="{{[['tap',[['toggle',[false,1]]]]]}}" bindtap="__e" class="data-v-71373bb2">确定</view></view><picker-view indicator-style="height: 36px;" value="{{[index]}}" data-event-opts="{{[['change',[['bindChange',['$event']]]]]}}" bindchange="__e" class="data-v-71373bb2"><picker-view-column class="data-v-71373bb2"><block s-for="{{choose_list}}" s-for-item="item" s-for-index="idx" s-key="name"><view class="{{['data-v-71373bb2','picker-view',[(newIndex===idx)?'sure-color':'',(newIndex===idx+1||newIndex===idx-1)?'cardinal-color':'',(newIndex===idx+2||newIndex===idx-2)?'even-color':'']]}}">{{item.name+''}}</view></block></picker-view-column></picker-view></view></view></block></app-layout>