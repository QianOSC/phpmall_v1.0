<app-layout vue-id="1" class="data-v-a6ba5f40" vue-slots="{{['default']}}"><view class="balance-detail-head cross-center data-v-a6ba5f40"><view class="dir-left-nowrap cross-center data-v-a6ba5f40"><view class="box-grow-0 key data-v-a6ba5f40">交易金额</view><block s-if="{{detail.type==1}}"><view class="box-grow-1 value red data-v-a6ba5f40">{{"+"+detail.money}}</view></block><block s-if="{{detail.type==2}}"><view class="box-grow-1 value blue data-v-a6ba5f40">{{"-"+detail.money}}</view></block></view></view><view class="balance-detail data-v-a6ba5f40"><view class="dir-left-nowrap info data-v-a6ba5f40"><view class="box-grow-0 key data-v-a6ba5f40">交易时间</view><view class="box-grow-1 value data-v-a6ba5f40">{{detail.created_at}}</view></view><view class="dir-left-nowrap info data-v-a6ba5f40"><view class="box-grow-0 key data-v-a6ba5f40">交易详情</view><view class="box-grow-1 value data-v-a6ba5f40">{{detail.desc}}</view></view><block s-if="{{detail.order_no}}"><view class="dir-left-nowrap info data-v-a6ba5f40"><view class="box-grow-0 key data-v-a6ba5f40">交易单号</view><view class="box-grow-1 value data-v-a6ba5f40">{{detail.order_no}}</view></view></block><block s-if="{{detail.order_refund_no}}"><view class="dir-left-nowrap info data-v-a6ba5f40"><view class="box-grow-0 key data-v-a6ba5f40">退款单号</view><view class="box-grow-1 value data-v-a6ba5f40">{{detail.order_refund_no}}</view></view></block></view></app-layout>