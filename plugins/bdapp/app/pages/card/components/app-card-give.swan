<block s-if="{{card}}"><view class="give data-v-dca4b1ba"><view class="content data-v-dca4b1ba" style="{{(contentStyle)}}"><view class="avatar dir-left-nowrap main-center cross-center data-v-dca4b1ba"><image src="{{card.avatar}}" class="data-v-dca4b1ba"></image></view><view class="nickname data-v-dca4b1ba">{{card.nickname}}</view><view class="title data-v-dca4b1ba">送你一张卡券</view><block s-if="{{card.status==2}}"><view class="pic dir-left-nowrap main-between cross-center data-v-dca4b1ba"><view class="card-title box-grow-0 data-v-dca4b1ba"><image src="{{card.pic_url}}" class="data-v-dca4b1ba"></image></view><view class="name t-omit-two box-grow-0 data-v-dca4b1ba">{{card.name}}</view><view class="box-grow-1 data-v-dca4b1ba"></view></view><view class="reset-time data-v-dca4b1ba">{{"剩余次数："+card.reset_number+"次"}}</view><view class="time data-v-dca4b1ba">{{"有效期："+card.start_time+" - "+card.end_time}}</view><view class="card-btn-group dir-left-nowrap main-center data-v-dca4b1ba"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="card-btn btn-0 box-grow-0 data-v-dca4b1ba" bindtap="__e"><app-image vue-id="1" img-src="{{card.img_back}}" width="100%" height="100%" class="data-v-dca4b1ba"></app-image></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="card-btn btn-0 box-grow-0 data-v-dca4b1ba" bindtap="__e"><app-image vue-id="2" img-src="{{card.img_share}}" width="100%" height="100%" class="data-v-dca4b1ba"></app-image></view></view><view class="data-v-dca4b1ba"><app-share-qr-code bind:input="__e" vue-id="3" title="生成卡券海报" poster-url="{{'/pages/poster/poster?card_id='+card.id}}" has-poster-nav="{{true}}" value="{{shareShow}}" data-event-opts="{{[['^input',[['__set_model',['','shareShow','$event',[]]]]]]}}" class="data-v-dca4b1ba"></app-share-qr-code></view></block><block s-else><view data-event-opts="{{[['tap',[['receive',['$event']]]]]}}" class="card-btn btn-1 data-v-dca4b1ba" bindtap="__e"><app-image vue-id="4" img-src="{{card.img_receive}}" width="100%" height="100%" class="data-v-dca4b1ba"></app-image></view></block><block s-if="{{modal.show}}"><view class="card-modal dir-top-nowrap cross-center main-center data-v-dca4b1ba"><view class="modal-content data-v-dca4b1ba" style="{{'background-image:'+('url('+card.img_finish_receiving+')')+';'}}"><view class="error data-v-dca4b1ba">{{modal.msg}}</view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="modal-btn data-v-dca4b1ba" bindtap="__e"></view></view></view></block></view><view class="data-v-dca4b1ba"><block s-if="{{recommendGoodsList&&recommendGoodsList.length}}"><app-related-suggestion-product vue-id="5" list="{{recommendGoodsList}}" text="您或许会喜欢" theme="{{getTheme}}" class="data-v-dca4b1ba"></app-related-suggestion-product></block></view></view></block>