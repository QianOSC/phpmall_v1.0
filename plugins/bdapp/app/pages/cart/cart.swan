<app-layout vue-id="1" overflow="{{false}}" class="data-v-62c74cc6" vue-slots="{{['default']}}"><view class="app-cart data-v-62c74cc6"><view class="app-announcement dir-left-nowrap main-between cross-center data-v-62c74cc6"><text class="app-announcement-text data-v-62c74cc6">商品库存有限，请尽快下单</text><app-form-id bind:click="__e" vue-id="{{('2')+','+('1')}}" data-event-opts="{{[['^click',[['editSwitch']]]]}}" class="data-v-62c74cc6" vue-slots="{{['default']}}"><text class="app-edit-text data-v-62c74cc6">{{editStatus?'完成':'编辑'}}</text></app-form-id></view><block s-for="{{listObj}}" s-for-item="mch" s-for-index="index" s-key="index"><block s-if="{{listObj.length>0}}"><view class="app-item data-v-62c74cc6"><app-shop-product vue-id="{{('3-'+index)+','+('1')}}" theme="{{getTheme}}" mch="{{mch}}" editStatus="{{editStatus}}" data-event-opts="{{[['^change',[['change']]],['^update',[['update']]],['^changeSingleRadio',[['changeSingleRadio']]],['^changeRadioAll',[['changeRadioAll']]]]}}" bind:change="__e" bind:update="__e" bind:changeSingleRadio="__e" bind:changeRadioAll="__e" class="data-v-62c74cc6"></app-shop-product></view></block></block><block s-if="{{listObj.length===0}}"><view class="no-cart data-v-62c74cc6"><view class="cart-icon data-v-62c74cc6"><image class="cart-image data-v-62c74cc6" src="../../static/image/icon/nav-icon-cart.png"></image></view><view class="cart-text data-v-62c74cc6">购物车还是空的哦</view></view></block><view class="data-v-62c74cc6"><app-empty-bottom vue-id="{{('4')+','+('1')}}" backgroundColor="#f7f7f7" botBool="{{botBool}}" height="{{$root.m0}}" class="data-v-62c74cc6"></app-empty-bottom></view><block s-if="{{!tabbarbool}}"><view style="{{'background:'+('white')+';'+('position:'+('fixed')+';')+('bottom:'+(0)+';')+('height:'+(getEmpty+'rpx')+';')+('width:'+('750rpx')+';')}}" class="data-v-62c74cc6"></view></block><view class="app-settlement dir-left-nowrap main-between cross-center data-v-62c74cc6" style="{{'bottom:'+((tabbarbool?BotHeight:getEmpty)+'rpx')+';'}}"><view class="app-radio dir-left-nowrap main-right cross-center data-v-62c74cc6"><app-radio vue-id="{{('5')+','+('1')}}" type="round" theme="{{getTheme}}" value="{{all}}" data-event-opts="{{[['^click',[['setALl']]],['^input',[['__set_model',['','all','$event',[]]]]]]}}" bind:click="__e" bind:input="__e" class="data-v-62c74cc6"></app-radio><text class="app-all-text data-v-62c74cc6">全选</text><block s-if="{{!editStatus}}"><text class="{{['app-price data-v-62c74cc6',getTheme+'-m-text '+getTheme]}}">{{"总计: ￥"+priceNum}}</text></block></view><block s-if="{{editStatus}}"><button class="{{['app-delete data-v-62c74cc6',submitDis?'delete-disabled':getTheme+'-m-text '+getTheme]}}" disabled="{{submitDis}}" data-event-opts="{{[['tap',[['deleteProduct',['$event']]]]]}}" bindtap="__e">删除</button></block><block s-else><button class="{{['app-settlement-button data-v-62c74cc6',submitDis?'disabled-background':getTheme+'-m-gradient-btn '+getTheme]}}" disabled="{{submitDis}}" data-event-opts="{{[['tap',[['settlement',['$event']]]]]}}" bindtap="__e">去结算</button></block></view></view></app-layout>