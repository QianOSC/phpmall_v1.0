<app-layout vue-id="1" vue-slots="{{['default']}}"><block s-if="{{card}}"><view class="page"><view class="main-card" style="{{'background-image:'+('url('+appImg.vip_card.default_card+')')+';'}}"><view class="name">{{card.name}}</view><view style="margin-bottom:24rpx;">开通超级会员可以享受以下权益</view><view class="dir-left-nowrap cross-center"><image style="width:42rpx;height:30rpx;margin-right:18rpx;" src="/static/image/icon/vip-card-coupon.png"></image><view style="margin-right:50rpx;">{{card.discount==0?'免费':card.discount+'折'}}</view><block s-if="{{card.is_free_delivery==1||true}}"><image style="width:38rpx;height:32rpx;margin-right:18rpx;" src="/static/image/icon/vip-card-delivery.png"></image><view style="margin-right:32rpx;">自营商品包邮</view></block><view data-event-opts="{{[['tap',[['navigateVipCardPrivilege',['$event']]]]]}}" style="color:#b17426;" bindtap="__e"><text>查看权益 ></text></view></view></view><block s-for="{{card.detail}}" s-for-item="item" s-for-index="index" s-key="index"><view data-event-opts="{{[['tap',[['active',[index]]]]]}}" class="{{['child-card dir-left-nowrap cross-center',item.active?'active':'']}}" bindtap="__e"><view class="box-grow-1"><view style="margin-bottom:12rpx;">{{item.name}}</view><view class="dir-left-nowrap" style="font-size:24rpx;"><block s-if="{{item.active}}"><view data-event-opts="{{[['tap',[['showDesc',['$0'],[[['card.detail','',index]]]]]]]}}" class="dir-left-nowrap cross-center" catchtap="__e"><view style="color:#ffb85d;margin-right:8rpx;font-size:22rpx;line-height:1.05;">使用说明</view><app-css-icon vue-id="{{('2-'+index)+','+('1')}}" icon="arrow-right" size="22" padding round color="#fff" background="#ffb85d"></app-css-icon></view></block><block s-else><view data-event-opts="{{[['tap',[['showDesc',['$0'],[[['card.detail','',index]]]]]]]}}" class="dir-left-nowrap cross-center" catchtap="__e"><view style="color:#999;margin-right:8rpx;font-size:22rpx;line-height:1.05;">使用说明</view><app-css-icon vue-id="{{('3-'+index)+','+('1')}}" icon="arrow-right" size="22" padding round color="#fff" background="#999"></app-css-icon></view></block></view></view><view class="{{['box-grow-0 dir-left-nowrap cross-center',theme]}}"><view class="{{['price',theme+' '+theme+'-m-text']}}" style="margin-right:24rpx;">{{'¥'+item.price+''}}</view><block s-if="{{item.active}}"><view class="{{['checkbox',theme+' '+theme+'-m-back '+theme+'-m-border']}}"><block s-if="{{item.active}}"><app-css-icon vue-id="{{('4-'+index)+','+('1')}}" icon="check" size="{{28}}" color="#fff" transform="translate(28%, -15%)"></app-css-icon></block></view></block><block s-else><view class="checkbox border-gray"></view></block></view></view></block><view class="submit-bar dir-left-nowrap"><view class="{{['box-grow-1 cross-center price',theme+' '+theme+'-m-text']}}"><block s-if="{{selectedCard}}">{{"开卡费用：¥"+selectedCard.price}}</block><block s-else>请选择需要开通的子卡</block></view><view data-event-opts="{{[['tap',[['setData',['$event']]]]]}}" class="{{['box-grow-0 cross-center submit-btn',theme+' '+theme+'-m-back']}}" bindtap="__e">提交</view></view><app-dialog vue-id="{{('5')+','+('1')}}" title="{{descDialog.title}}" content="{{descDialog.content}}" confirm-text="我知道了" visible="{{descDialog.visible}}" data-event-opts="{{[['^updateVisible',[['__set_sync',['$0','visible','$event'],['descDialog']]]]]}}" bind:updateVisible="__e"></app-dialog></view></block></app-layout>