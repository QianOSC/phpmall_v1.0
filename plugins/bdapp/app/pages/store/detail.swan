<app-layout vue-id="1" class="data-v-811abdb4" vue-slots="{{['default']}}"><app-swiper vue-id="{{('2')+','+('1')}}" height="{{360}}" list="{{list.pic_url}}" autoplay="{{true}}" name="pic_url" class="data-v-811abdb4"></app-swiper><view class="head data-v-811abdb4"><view class="name data-v-811abdb4">{{list.name}}</view><view class="dir-left-nowrap cross-center score data-v-811abdb4"><view class="box-grow-0 data-v-811abdb4">评分：</view><block s-for="{{list.score}}" s-for-item="i" s-for-index="__i0__" s-key="*this"><icon class="box-grow-0 image data-v-811abdb4"></icon></block></view><view class="time data-v-811abdb4">{{"营业时间："+list.business_hours}}</view></view><view class="cross-center tel data-v-811abdb4"><view class="box-grow-0 data-v-811abdb4">电话：</view><view class="box-grow-1 info data-v-811abdb4">{{list.mobile}}</view><image class="box-grow-0 data-v-811abdb4" src="/static/image/icon/store-tel.png" data-event-opts="{{[['tap',[['mobile',['$event']]]]]}}" bindtap="__e"></image></view><view class="cross-top address data-v-811abdb4"><view class="box-grow-0 data-v-811abdb4">地址：</view><view class="box-grow-1 info data-v-811abdb4">{{list.address}}</view><image class="box-grow-0 data-v-811abdb4" src="/static/image/icon/store-navigate.png" data-event-opts="{{[['tap',[['goto',['$event']]]]]}}" bindtap="__e"></image></view><view class="bg-line data-v-811abdb4"></view><view class="content cross-center data-v-811abdb4">店铺介绍</view><view class="end data-v-811abdb4"><app-rich-text vue-id="{{('3')+','+('1')}}" content="{{list.description}}" class="data-v-811abdb4"></app-rich-text></view></app-layout>