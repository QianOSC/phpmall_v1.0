<app-layout vue-id="1" class="data-v-3d57adf6" vue-slots="{{['default']}}"><image class="banner data-v-3d57adf6" src="{{setting.banner}}"></image><view class="nav-area data-v-3d57adf6"><view class="{{['nav main-center cross-center data-v-3d57adf6',getTheme+'-m-back '+getTheme]}}"><block s-for="{{tabList}}" s-for-item="item" s-for-index="index" s-key="id"><view data-event-opts="{{[['tap',[['tabStatus',['$0',index],[[['tabList','id',item.id]]]]]]]}}" class="nav-item data-v-3d57adf6" bindtap="__e"><text class="data-v-3d57adf6">{{item.name}}</text></view></block><view class="{{['active data-v-3d57adf6',getTheme+'-m-text '+getTheme]}}" animation="{{animationData}}">{{activeTabName}}</view></view></view><view class="placeholder data-v-3d57adf6"></view><block s-if="{{list.length>0}}"><view class="data-v-3d57adf6"><block s-for="{{list}}" s-for-item="item" s-for-index="__i0__" s-key="id"><block s-if="{{item.goods_count>0}}"><view class="list data-v-3d57adf6"><view class="{{['title main-center dir-top-nowrap data-v-3d57adf6',getTheme+'-m-gradient '+getTheme]}}"><view class="title-text t-omit data-v-3d57adf6">{{item.title+''}}<image class="hot-icon data-v-3d57adf6" src="./../image/hot.png"></image></view><block s-if="{{item.condition>0}}"><view class="sec-title-text t-omit data-v-3d57adf6"><block s-if="{{item.num-item.condition_count>0}}"><text class="data-v-3d57adf6">{{"还差"+(item.num-item.condition_count)+(item.condition==1?'人':'件商品')+"成团"}}</text></block><block s-else><text class="data-v-3d57adf6">已成团</text></block>，已<block s-if="{{item.condition==1}}"><text class="data-v-3d57adf6">参</text></block>{{"团"+(item.condition_count?item.condition_count:'0')+(item.condition==1?'人':'件')+''}}</view></block></view><view class="{{['status-text data-v-3d57adf6',getTheme+'-m-text '+getTheme]}}"><view class="{{['status-bg data-v-3d57adf6',getTheme+'-m-back '+getTheme]}}"></view>{{''+item.statusText+''}}</view><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['list','id',item.id]]]]]]]}}" class="goods-list data-v-3d57adf6" bindtap="__e"><view class="dir-left-nowrap data-v-3d57adf6"><block s-for="{{item.communityGoods_4}}" s-for-item="goods" s-for-index="__i1__" s-key="id"><view class="goods data-v-3d57adf6"><image mode="aspectFill" src="{{goods.cover_pic}}" class="data-v-3d57adf6"></image></view></block></view><image class="right-arrow data-v-3d57adf6" src="/static/image/icon/arrow-right.png"></image></view><view class="main-between cross-center other data-v-3d57adf6"><view class="data-v-3d57adf6">{{"共"+item.goods_count+"件商品"}}</view></view></view></block></block></view></block><block s-else><view class="empty-box data-v-3d57adf6"><block s-if="{{!loading}}"><image src="/static/image/no-goods.png" class="data-v-3d57adf6"></image></block><block s-if="{{!loading}}"><label class="_span data-v-3d57adf6">{{"暂无"+(activeTab==0?'未开始':activeTab==1?'进行中':activeTab==2?'已结束':'')+"活动"}}</label></block></view></block></app-layout>