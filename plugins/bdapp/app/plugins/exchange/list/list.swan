<app-layout vue-id="1" class="data-v-1e5a9a54" vue-slots="{{['default']}}"><app-tab-nav vue-id="{{('2')+','+('1')}}" setTop="0" border="{{false}}" shadow="{{false}}" height="100" tabList="{{tabList}}" padding="0" activeItem="{{activeTab}}" theme="{{getTheme}}" data-event-opts="{{[['^click',[['tabStatus']]]]}}" bind:click="__e" class="data-v-1e5a9a54"></app-tab-nav><block s-if="{{activeTab=='0'}}"><view class="goods dir-left-wrap data-v-1e5a9a54"><block s-if="{{list.length>0}}"><view style="width:100%;" class="data-v-1e5a9a54"><app-product-list vue-id="{{('3')+','+('1')}}" theme-object="{{themeObject}}" sign="exchange" theme="{{getTheme}}" listStyle="{{listStyle}}" goods_list="{{list}}" data-event-opts="{{[['^routeGo',[['jump']]]]}}" bind:routeGo="__e" class="data-v-1e5a9a54"></app-product-list></view></block><block s-if="{{list.length==0}}"><view style="padding-top:30%;width:100%;" class="data-v-1e5a9a54"><app-no-goods vue-id="{{('4')+','+('1')}}" title="暂无可购买的礼品卡" background="#f7f7f7" class="data-v-1e5a9a54"></app-no-goods></view></block></view></block><block s-if="{{activeTab=='1'}}"><view class="data-v-1e5a9a54"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><view class="item data-v-1e5a9a54"><view class="dir-left-nowrap cross-center status data-v-1e5a9a54"><view class="item-cover data-v-1e5a9a54"><app-image vue-id="{{('5-'+index)+','+('1')}}" img-src="{{item.cover_pic}}" width="140rpx" height="140rpx" border-radius="8rpx 8rpx 0 0" class="data-v-1e5a9a54"></app-image></view><view class="t-omit-two item-name data-v-1e5a9a54">{{item.name}}</view></view><view class="item-time main-between cross-center data-v-1e5a9a54"><view class="data-v-1e5a9a54">{{"购买时间："+item.created_at}}</view><block s-if="{{item.status=='unused'}}"><view data-event-opts="{{[['tap',[['apply',['$0'],[[['list','',index]]]]]]]}}" class="{{['rewards-submit data-v-1e5a9a54',getTheme+'-m-back '+getTheme]}}" bindtap="__e">立即使用</view></block><block s-else><block s-if="{{item.status=='used'}}"><view data-event-opts="{{[['tap',[['apply',['$0'],[[['list','',index]]]]]]]}}" class="rewards-submit over data-v-1e5a9a54" bindtap="__e">已使用</view></block><block s-else><view data-event-opts="{{[['tap',[['apply',['$0'],[[['list','',index]]]]]]]}}" class="rewards-submit over data-v-1e5a9a54" bindtap="__e">已失效</view></block></block></view></view></block><block s-if="{{list.length==0}}"><view style="padding-top:30%;" class="data-v-1e5a9a54"><app-no-goods vue-id="{{('6')+','+('1')}}" title="暂无已获得的礼品卡" background="#f7f7f7" class="data-v-1e5a9a54"></app-no-goods></view></block></view></block><block s-if="{{setting.is_to_exchange==1}}"><view data-event-opts="{{[['tap',[['toIndex',['$event']]]]]}}" class="{{['jump-dialog cross-center dir-top-nowrap main-center data-v-1e5a9a54',''+(tabbarbool?'tabbarbool':'')]}}" bindtap="__e"><image src="{{setting.to_exchange_pic}}" class="data-v-1e5a9a54"></image><view class="data-v-1e5a9a54">兑换中心</view></view></block></app-layout>