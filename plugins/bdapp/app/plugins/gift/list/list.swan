<app-layout vue-id="1" vue-slots="{{['default']}}"><block s-if="{{loading}}"><view class="list"><view class="page-width search"><search vue-id="{{('2')+','+('1')}}"></search></view><view class="page-width sort-rule"><app-sort-rule vue-id="{{('3')+','+('1')}}" theme="{{theme}}" sign="gift" route="/plugins/gift/search/search" data-event-opts="{{[['^sort',[['set_sort']]],['^setStyle',[['setStyle']]]]}}" bind:sort="__e" bind:setStyle="__e"></app-sort-rule></view><view class="page-width product-list"><product-list vue-id="{{('4')+','+('1')}}" goods_list="{{goods_list}}" theme="{{theme}}" sign="gift" listStyle="{{listStyle}}" themeObject="{{themeObject}}" data-event-opts="{{[['^routeGo',[['routeGo']]]]}}" bind:routeGo="__e"></product-list></view><block s-if="{{goods_list.length===0}}"><view class="page-width no-goods"><app-no-goods vue-id="{{('5')+','+('1')}}" background="#f7f7f7"></app-no-goods></view></block></view></block></app-layout>