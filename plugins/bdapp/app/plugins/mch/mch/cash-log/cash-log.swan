<app-layout vue-id="1" class="data-v-0723cfd0" vue-slots="{{['default']}}"><view class="list data-v-0723cfd0"><view class="account-date main-center cross-center data-v-0723cfd0"><image class="account-icon data-v-0723cfd0" src="../../../../static/image/icon/arrow-left.png" data-event-opts="{{[['tap',[['dateLess',['$event']]]]]}}" bindtap="__e"></image><picker mode="date" value="{{date}}" fields="month" data-event-opts="{{[['change',[['dateChange',['$event']]]]]}}" bindchange="__e" class="data-v-0723cfd0"><view class="data-v-0723cfd0">{{date_a}}</view></picker><image class="account-icon data-v-0723cfd0" src="../../../../static/image/icon/arrow-right.png" data-event-opts="{{[['tap',[['datePlus',['$event']]]]]}}" bindtap="__e"></image></view><block s-if="{{!list||!list.length}}"><view class="no-content data-v-0723cfd0">暂无记录</view></block><block s-else><view class="account-list data-v-0723cfd0"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><view class="account-item dir-left-nowrap cross-center data-v-0723cfd0"><view class="box-grow-1 left data-v-0723cfd0"><block s-if="{{item.status==='0'}}"><view class="data-v-0723cfd0">待审核</view></block><block s-else><block s-if="{{item.status==='1'&&item.transfer_status==='0'}}"><view class="data-v-0723cfd0">待打款</view></block><block s-else><block s-if="{{item.status==='1'&&item.transfer_status==='1'}}"><view class="green data-v-0723cfd0">已打款</view></block><block s-else><block s-if="{{item.status==='1'&&item.transfer_status==='2'}}"><view class="red data-v-0723cfd0">拒绝打款</view></block><block s-else><view class="red data-v-0723cfd0">审核未通过</view></block></block></block></block><view class="created data-v-0723cfd0">{{item.created_at}}</view></view><view class="add-money data-v-0723cfd0">{{item.money}}</view></view></block></view></block></view></app-layout>