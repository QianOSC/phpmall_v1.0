<app-layout vue-id="1" class="data-v-591cb4fc" vue-slots="{{['default']}}"><view class="data-v-591cb4fc"><view class="input-list data-v-591cb4fc"><view class="dir-left-nowrap input-item data-v-591cb4fc"><view class="label data-v-591cb4fc">收货人</view><input name="name" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e" class="data-v-591cb4fc"/></view><view class="dir-left-nowrap input-item data-v-591cb4fc"><view class="label data-v-591cb4fc">联系电话</view><input name="mobile" type="number" data-event-opts="{{[['input',[['__set_model',['','mobile','$event',[]]]]]]}}" value="{{mobile}}" bindinput="__e" class="data-v-591cb4fc"/></view><view class="main-between input-item data-v-591cb4fc"><text class="label data-v-591cb4fc">所在地区</text><app-area-picker vue-id="{{('2')+','+('1')}}" ids="{{ids}}" data-event-opts="{{[['^customevent',[['areaEvent']]]]}}" bind:customevent="__e" class="data-v-591cb4fc"></app-area-picker></view><view class="dir-left-nowrap input-item data-v-591cb4fc"><view class="label data-v-591cb4fc">详细地址</view><input name="detail" placeholder="请输入详细地址(最多50字)" maxlength="50" data-event-opts="{{[['input',[['__set_model',['','detail','$event',[]]]]]]}}" value="{{detail}}" bindinput="__e" class="data-v-591cb4fc"/></view></view><button data-event-opts="{{[['tap',[['saveAddress',['$event']]]]]}}" class="address-bottom data-v-591cb4fc" bindtap="__e">{{response.data.list[0].address?'确认修改地址':'确定'}}</button></view></app-layout>