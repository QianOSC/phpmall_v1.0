<app-layout vue-id="1" class="data-v-c0649d34" vue-slots="{{['default']}}"><block s-if="{{id}}"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="screen data-v-c0649d34" bindtap="__e"></view></block><view class="search main-between data-v-c0649d34"><block s-if="{{!toSearch}}"><view data-event-opts="{{[['tap',[['beSearch',['$event']]]]]}}" class="main-center search-content cross-center data-v-c0649d34" bindtap="__e"><image class="search-icon data-v-c0649d34" src="/static/image/icon/icon-search.png"></image><text class="search-text data-v-c0649d34">搜索</text></view></block><block s-else><view class="dir-left-nowrap data-v-c0649d34"><input class="box-grow-1 t-small search-content data-v-c0649d34" focus confirm-type="search" data-event-opts="{{[['blur',[['beSearch',['$event']]]],['confirm',[['reload',['$event']]]],['input',[['__set_model',['','keyword','$event',[]]]]]]}}" value="{{keyword}}" bindblur="__e" bindconfirm="__e" bindinput="__e"/></view></block><view data-event-opts="{{[['tap',[['toAdd',['$event']]]]]}}" bindtap="__e" class="data-v-c0649d34"><view class="add-goods dir-left-nowrap data-v-c0649d34"><image src="./../../image/add.png" class="data-v-c0649d34"></image><view class="data-v-c0649d34">添加商品</view></view></view></view><app-tab-nav vue-id="{{('2')+','+('1')}}" setTop="88" tabList="{{tabList}}" padding="0" activeItem="{{activeTab}}" theme="default" data-event-opts="{{[['^click',[['tabStatus']]]]}}" bind:click="__e" class="data-v-c0649d34"></app-tab-nav><view class="top-place data-v-c0649d34"></view><view class="data-v-c0649d34"><block s-for="{{list}}" s-for-item="item" s-for-index="__i0__" s-key="id"><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="msg-item data-v-c0649d34" bindtap="__e"><image class="msg-img data-v-c0649d34" src="{{item.cover_pic}}"></image><block s-if="{{item.goods_stock==0&&appSetting.is_show_stock=='1'}}"><view class="out-dialog data-v-c0649d34"><image src="{{appSetting.is_use_stock=='1'?appImg.plugins_out:appSetting.sell_out_pic}}" class="data-v-c0649d34"></image></view></block><view class="t-omit-two goods-name data-v-c0649d34">{{item.name}}</view><view class="goods-price data-v-c0649d34">{{"¥"+item.price}}</view><view class="goods-num data-v-c0649d34">{{"库存:"+item.goods_stock}}</view><image class="more-handle data-v-c0649d34" src="./../../image/more-handle.png" data-event-opts="{{[['tap',[['showMenu',['$0'],[[['list','id',item.id,'id']]]]]]]}}" catchtap="__e"></image><block s-if="{{item.id==id}}"><view class="main-between more data-v-c0649d34"><view data-event-opts="{{[['tap',[['toEdit',['$0'],[[['list','id',item.id,'id']]]]]]]}}" catchtap="__e" class="data-v-c0649d34"><view class="more-item data-v-c0649d34"><image src="./../../image/goods-edit.png" class="data-v-c0649d34"></image><view class="data-v-c0649d34">编辑</view></view></view><block s-if="{{item.status==1}}"><view data-event-opts="{{[['tap',[['toSwitch',[0,'$0'],[[['list','id',item.id,'id']]]]]]]}}" catchtap="__e" class="data-v-c0649d34"><view class="more-item data-v-c0649d34"><image src="./../../image/goods-xiajia.png" class="data-v-c0649d34"></image><view class="data-v-c0649d34">下架</view></view></view></block><block s-if="{{item.status==0&&is_goods_audit==0}}"><view data-event-opts="{{[['tap',[['toSwitch',[1,'$0'],[[['list','id',item.id,'id']]]]]]]}}" catchtap="__e" class="data-v-c0649d34"><view class="more-item data-v-c0649d34"><image style="transform:rotate(180deg);" src="./../../image/goods-xiajia.png" class="data-v-c0649d34"></image><view class="data-v-c0649d34">上架</view></view></view></block><block s-if="{{(item.mch_goods.status==0||item.mch_goods.status==3)&&item.status==0&&is_goods_audit==1}}"><view data-event-opts="{{[['tap',[['toSwitch',[2,'$0'],[[['list','id',item.id,'id']]]]]]]}}" catchtap="__e" class="data-v-c0649d34"><view class="more-item data-v-c0649d34"><image style="transform:rotate(180deg);" src="./../../image/goods-xiajia.png" class="data-v-c0649d34"></image><view class="data-v-c0649d34">申请上架</view></view></view></block><view data-event-opts="{{[['tap',[['toDelete',['$0'],[[['list','id',item.id,'id']]]]]]]}}" catchtap="__e" class="data-v-c0649d34"><view class="more-item data-v-c0649d34"><image src="./../../image/goods-del.png" class="data-v-c0649d34"></image><view class="data-v-c0649d34">删除</view></view></view></view></block><block s-if="{{item.id==id}}"><view class="and-more data-v-c0649d34"></view></block></view></block></view><block s-if="{{list.length==0}}"><view class="no-tip data-v-c0649d34"><image src="{{adminImg.no_goods}}" class="data-v-c0649d34"></image><view class="data-v-c0649d34">没有任何商品哦~</view></view></block><block s-if="{{is_switch||is_delete||apply_up}}"><view class="dialog data-v-c0649d34"><block s-if="{{is_switch||is_delete||apply_up}}"><view class="dialog-item data-v-c0649d34"><view class="dialog-title data-v-c0649d34">提示</view><block s-if="{{is_switch&&status==0}}"><view class="dialog-txt data-v-c0649d34">是否下架该商品</view></block><block s-if="{{is_switch&&status==1}}"><view class="dialog-txt data-v-c0649d34">是否上架该商品</view></block><block s-if="{{apply_up}}"><view class="dialog-txt data-v-c0649d34">是否申请上架该商品</view></block><block s-if="{{is_delete}}"><view class="dialog-txt data-v-c0649d34">是否删除该商品</view></block><view class="main-center btn-area data-v-c0649d34"><view data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" class="submit-btn data-v-c0649d34" bindtap="__e">取消</view><view class="line data-v-c0649d34"></view><block s-if="{{is_switch}}"><view data-event-opts="{{[['tap',[['goods_switch',['$event']]]]]}}" class="submit-btn be-submit data-v-c0649d34" bindtap="__e">确认</view></block><block s-if="{{is_delete}}"><view data-event-opts="{{[['tap',[['goods_destroy',['$event']]]]]}}" class="submit-btn be-submit data-v-c0649d34" bindtap="__e">确认</view></block><block s-if="{{apply_up}}"><view data-event-opts="{{[['tap',[['applyStatus',['$event']]]]]}}" class="submit-btn be-submit data-v-c0649d34" bindtap="__e">确认</view></block></view></view></block></view></block></app-layout>