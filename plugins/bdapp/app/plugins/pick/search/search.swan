<app-layout vue-id="1" class="data-v-203c3553" vue-slots="{{['default']}}"><view class="search data-v-203c3553"><view class="search-input dir-left-nowrap cross-center main-center data-v-203c3553"><input class="input data-v-203c3553" type="text" data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['__set_model',['','keyword','$event',[]]]]]]}}" value="{{keyword}}" bindconfirm="__e" bindinput="__e"/><text data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="search-text data-v-203c3553" bindtap="__e">搜索</text><block s-if="{{keyword.length>0}}"><image class="delete data-v-203c3553" src="/static/image/icon/delete-yuan.png" data-event-opts="{{[['tap',[['deleteKeyword',['$event']]]]]}}" bindtap="__e"></image></block></view><block s-if="{{!isSearch&&historyList.length>0}}"><view class="history data-v-203c3553"><view class="dir-left-nowrap cross-center title data-v-203c3553"><view class="box-grow-1 data-v-203c3553">历史搜索</view><image class="box-grow-0 data-v-203c3553" src="/static/image/icon/delete.png" data-event-opts="{{[['tap',[['deleteHistory',['$event']]]]]}}" bindtap="__e"></image></view><view class="dir-left-wrap history-list data-v-203c3553"><block s-for="{{historyList}}" s-for-item="item" s-for-index="index" s-key="index"><view data-event-opts="{{[['tap',[['historyClick',['$0'],[[['historyList','',index,'keyword']]]]]]]}}" class="box-grow-0 cross-center item data-v-203c3553" bindtap="__e">{{item.keyword+''}}</view></block></view></view></block><view class="list data-v-203c3553"><block s-for="{{list}}" s-for-item="item" s-for-index="__i0__" s-key="id"><view data-event-opts="{{[['tap',[['navigator',['/plugins/pick/detail/detail?goods_id='+item.id]]]]]}}" class="item dir-left-nowrap data-v-203c3553" bindtap="__e"><block s-if="{{item.goods_stock==0}}"><view class="out-dialog data-v-203c3553"><image src="{{appSetting.is_use_stock=='1'?appImg.plugins_out:appSetting.sell_out_pic}}" class="data-v-203c3553"></image></view></block><image class="image data-v-203c3553" src="{{item.cover_pic}}"></image><view class="content dir-top-nowrap main-between data-v-203c3553"><text class="name t-omit-two data-v-203c3553">{{item.name}}</text><view class="information dir-left-nowrap main-between cross-bottom data-v-203c3553"><view class="num data-v-203c3553"><view class="value data-v-203c3553"><text class="{{['price data-v-203c3553',getTheme+'-m-text '+getTheme]}}">{{"￥"+item.price}}</text><text class="original_price data-v-203c3553">{{"￥"+item.original_price}}</text></view><text class="sales data-v-203c3553">{{item.sales}}</text></view><view class="data-v-203c3553"><view class="{{['go-button data-v-203c3553',getTheme+'-m-back '+getTheme]}}">去凑单</view></view></view></view></view></block><block s-if="{{isSearch}}"><view class="goods-list data-v-203c3553"><block s-if="{{list.length<=0}}"><view class="main-center no-result data-v-203c3553"><view class="dir-left-nowrap cross-center data-v-203c3553"><image class="box-grow-0 empty data-v-203c3553" src="/static/image/icon/empty.png"></image><view class="box-grow-1 data-v-203c3553"><view class="data-v-203c3553">抱歉，没有相关商品</view></view></view></view></block></view></block></view></view></app-layout>