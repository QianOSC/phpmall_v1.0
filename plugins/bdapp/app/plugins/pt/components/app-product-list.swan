<view class="data-v-cd730b7e"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><view data-event-opts="{{[['tap',[['route_go',['$0'],[[['list','',index]]]]]]]}}" class="{{['bd-item dir-left-nowrap data-v-cd730b7e',(index!==list.length-1)?'bd-item-border':'']}}" bindtap="__e"><block s-if="{{item.goods_stock===0&&appSetting.is_show_stock===1}}"><view class="out-dialog data-v-cd730b7e"><image src="{{appSetting.is_use_stock==1?appImg.plugins_out:appSetting.sell_out_pic}}" class="data-v-cd730b7e"></image></view></block><image class="bd-image data-v-cd730b7e" src="{{item.cover_pic}}"></image><view class="bd-content dir-top-nowrap main-between cross-top data-v-cd730b7e"><view class="data-v-cd730b7e"><view class="bd-name u-line-2 data-v-cd730b7e">{{item.name}}</view><view class="{{['app-group data-v-cd730b7e',themeObject.color]}}">{{"已团"+item.sales+"件"}}</view></view><view class="app-price-button dir-left-nowrap main-between cross-bottom data-v-cd730b7e"><view class="price dir-top-nowrap main-right data-v-cd730b7e"><block s-if="{{item.is_level===1&&item.level_price!=-1}}"><view class="member-price data-v-cd730b7e"><app-member-price vue-id="{{'1-'+index}}" theme="{{themeObject}}" price="{{item.level_price}}" class="data-v-cd730b7e"></app-member-price></view></block><block s-if="{{item.vip_card_appoint.discount}}"><view class="sup-vip data-v-cd730b7e"><app-sup-vip vue-id="{{'2-'+index}}" is_vip_card_user="{{item.vip_card_appoint.is_vip_card_user}}" discount="{{item.vip_card_appoint.discount}}" class="data-v-cd730b7e"></app-sup-vip></view></block><view class="app-price dir-left-nowrap cross-bottom data-v-cd730b7e"><block s-if="{{item.price>0}}"><text class="{{['app-symbol app-now-price data-v-cd730b7e',themeObject.color]}}">{{item.price}}</text></block><block s-else><text class="{{['app-now-price data-v-cd730b7e',themeObject.color]}}">免费</text></block><text class="app-symbol app-old-price data-v-cd730b7e">{{item.original_price}}</text></view></view><block s-if="{{item.goods_stock>0}}"><view class="{{['app-button data-v-cd730b7e',themeObject.theme==='a'?'default-button':themeObject.back]}}">去开团</view></block><block s-else><view class="app-button data-v-cd730b7e" style="background:#DCDCDC;color:#fff;">已售罄</view></block></view></view></view></block></view>