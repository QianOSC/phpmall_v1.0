<app-layout vue-id="1" class="data-v-9933e024" vue-slots="{{['default']}}"><image class="bg data-v-9933e024" src="{{region.detail}}"></image><view class="info data-v-9933e024"><image class="avatar data-v-9933e024" src="{{detail.avatar}}"></image><block s-if="{{detail.level_up_log.status==1}}"><image class="success-img data-v-9933e024" src="{{bonus.success}}"></image></block><block s-if="{{detail.level_up_log.status==1}}"><view class="update main-between cross-center data-v-9933e024"><text class="data-v-9933e024">{{"恭喜您已升级成为"+detail.province_text+'的'}}<block s-if="{{detail.level==1}}"><text class="data-v-9933e024">省代理</text></block><block s-if="{{detail.level==2}}"><text class="data-v-9933e024">市代理</text></block></text></view></block><block s-if="{{detail.is_up==1}}"><view class="update main-between cross-center data-v-9933e024"><block s-if="{{detail.level_up_log.status==0}}"><view class="data-v-9933e024">{{"正在申请成为"+detail.province_text+"的"+detail.level_up.level_desc}}</view></block><block s-else><view class="data-v-9933e024">{{"可升级为"+detail.province_text+"的"+detail.level_up.level_desc}}</view></block><view data-event-opts="{{[['tap',[['toUpdate',['$event']]]]]}}" class="update-btn data-v-9933e024" bindtap="__e">{{detail.level_up_log.status==0?'去查看':'去升级'}}</view></view></block><view class="title main-center cross-center data-v-9933e024"><image src="./../image/title-left.png" class="data-v-9933e024"></image><view class="data-v-9933e024">代理级别</view><image src="./../image/title-right.png" class="data-v-9933e024"></image></view><view class="level-info data-v-9933e024"><block s-if="{{detail.level==1}}"><text class="data-v-9933e024">省代理</text></block><block s-if="{{detail.level==2}}"><text class="data-v-9933e024">市代理</text></block><block s-if="{{detail.level==3}}"><text class="data-v-9933e024">区/县代理</text></block><text class="placeholder data-v-9933e024">|</text><block s-if="{{detail.bonus_rate}}"><text class="data-v-9933e024">{{(setting.form.rate?setting.form.rate:'分红比例')+detail.bonus_rate+"%"}}</text></block></view><view class="title main-center cross-center data-v-9933e024"><image src="./../image/title-left.png" class="data-v-9933e024"></image><view class="data-v-9933e024">代理地区</view><image src="./../image/title-right.png" class="data-v-9933e024"></image></view><view class="data-v-9933e024"><text class="data-v-9933e024">{{detail.province_text}}</text><block s-if="{{detail.level>2&&detail.city_text}}"><text class="data-v-9933e024"><text class="placeholder data-v-9933e024">-</text>{{''+detail.city_text+''}}</text></block></view><block s-if="{{detail.level>1&&!(detail.relation.length==1&&detail.relation[0].name==detail.province_text)}}"><view class="relation-list data-v-9933e024"><block s-for="{{detail.relation}}" s-for-item="item" s-for-index="index" s-key="*this"><text class="data-v-9933e024"><block s-if="{{index>0}}"><text class="placeholder data-v-9933e024">/</text></block>{{''+item.name+''}}</text></block></view></block></view></app-layout>