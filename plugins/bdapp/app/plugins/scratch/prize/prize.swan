<app-layout vue-id="1" class="data-v-4e7b48d2" vue-slots="{{['default']}}"><block s-if="{{!list||!list.length}}"><view class="no-content data-v-4e7b48d2">暂无中奖记录</view></block><block s-for="{{list}}" s-for-item="v" s-for-index="k" s-key="k"><block class="data-v-4e7b48d2"><view class="dir-left-nowrap line cross-center data-v-4e7b48d2"><view class="dir-top-nowrap box-grow-1 main-center data-v-4e7b48d2"><view class="name t-omit data-v-4e7b48d2">{{v.name}}</view><view class="time data-v-4e7b48d2">{{v.created_at}}</view></view><view class="box-grow-0 data-v-4e7b48d2"><block s-if="{{v.status==1&&v.type==4}}"><app-button vue-id="{{('2-'+k)+','+('1')}}" background="#FFFFFF" height="56" width="164" color="#FF4544" font-size="28" round data-event-opts="{{[['^click',[['submit',['$0'],[[['list','',k]]]]]]]}}" bind:click="__e" class="data-v-4e7b48d2" vue-slots="{{['default']}}">立即兑换</app-button></block><block s-if="{{v.status==2&&v.type==4}}"><app-button vue-id="{{('3-'+k)+','+('1')}}" background="#CDCDCD" height="56" width="164" color="#FFFFFF" font-size="28" disabled round class="data-v-4e7b48d2" vue-slots="{{['default']}}">已兑换</app-button></block><block s-if="{{v.status==2&&v.type!=4}}"><app-button vue-id="{{('4-'+k)+','+('1')}}" background="#CDCDCD" height="56" width="164" color="#FFFFFF" font-size="28" disabled round class="data-v-4e7b48d2" vue-slots="{{['default']}}">已发放</app-button></block></view></view></block></block></app-layout>